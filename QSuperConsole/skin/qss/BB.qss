/* ******************QMenu****************** */
QMenu#titleMenu{
    color: white;
    background-color: qlineargradient(spread:pad, x1:-1, y1:0, x2:1 y2:0, stop:0 rgba(8, 148, 241, 255), stop:1 rgba(14, 142, 231, 255));
    margin: 2px; 
}

QMenu#titleMenu:item {
    height: 23px;
    padding: 0px 25px 0px 20px;
    background: qlineargradient(spread:pad, x1:-1, y1:0, x2:1 y2:0, stop:0 rgba(8, 148, 241, 255), stop:1 rgba(14, 142, 231, 255));
}

QMenu#titleMenu:item:selected {
    background-color: rgb(62, 165, 250);
    border-left: 5px solid rgba(8, 148, 241, 255);
}

QMenu#titleMenu:icon:checked { /* appearance of a 'checked' icon */
    background: rgb(62, 165, 250);
    border: 1px inset 6gray;
    position: absolute;
    top: 1px;
    right: 1px;
    bottom: 1px;
    left: 1px;
 }

QMenu#titleMenu:item:disabled
{
    background: rgb(125, 125, 125);
    border: 1px inset gray;
    position: absolute;
    top: 1px;
    right: 1px;
    bottom: 1px;
    left: 1px;
}

QMenu#titleMenu:separator {
    height: 2px;
    background: lightblue;
    margin-left: 10px;
    margin-right: 5px;
}

QMenu#titleMenu:indicator {
    width: 13px;
    height: 13px;
}
